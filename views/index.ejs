<%- include('./partials/header.ejs') %>
    <h1>Retro Games Classic</h1>
    <ul>
       
        <% for (let i = 0; i < snes.length; i++) { %>
            <li>
            <a href="/snes/<%=snes[i].id %>">
                <%=snes[i].title %>
                <img src=<%= snes[i].imag %> />
            </a>
                <form action="/snes/<%=snes[i].id %>?_method=DELETE" method="POST">
                    <input type="submit" value="DELETE" />
                </form>
            </li>
        <% } %>
    </ul>
</body>
</html>