<%- include('./partials/header.ejs') %>
    <h1>SNES Classic Games</h1>
    <div>
        <ul>
       
            <% for (let i = 0; i < snes.length; i++) { %>
                <li>
                <a href="/snes/<%=snes[i].id %>">
                    <%=snes[i].title %>
                    
                </a>
                </li>
                <img class="images" src=<%= snes[i].img %> />
                    <form action="/snes/<%=snes[i].id %>?_method=DELETE" method="POST">
                        <input type="submit" value="DELETE" />
                    </form>
            <% } %>
            
        </ul>
    </div>
   
</body>
</html>